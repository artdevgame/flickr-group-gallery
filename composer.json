                "version": "dev-master",
                "source": {
                    "url": "https://github.com/dan-coulter/phpflickr",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],

    "require": {
        "dan-coulter/phpflickr": "dev-master"
    },

    "autoload": {
